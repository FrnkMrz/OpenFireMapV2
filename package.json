{
  "name": "openfiremapv2",
  "version": "1.0.0",
  "description": "OpenFireMapV2 is a personal, non-commercial project focused on learning Git, GitHub, and modern CI/CD workflows, using OpenStreetMap (OSM) data and ideas inspired by the original OpenFireMap.",
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "node ./scripts/build.mjs",
    "build:css": "tailwindcss -c tailwind.config.js -i ./src/input.css -o ./docs/assets/css/main.css --minify",
    "watch:css": "tailwindcss -c tailwind.config.js -i ./src/input.css -o ./docs/assets/css/main.css --watch",
    "build": "node ./scripts/build.mjs && npm run build:css",
    "test": "echo \"no tests\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrnkMrz/OpenFireMapV2.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/FrnkMrz/OpenFireMapV2/issues"
  },
  "homepage": "https://github.com/FrnkMrz/OpenFireMapV2#readme",
  "devDependencies": {
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  }
}
